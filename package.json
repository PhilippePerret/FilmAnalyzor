    "electron": "^9.4.0"
  },
  "dependencies": {
    "adm-zip": "^0.4.13",
    "electron-log": "^3.0.5",
    "glob": "^7.1.3",
    "jquery": ">=3.4.1",
    "js-yaml": "^3.13.1"
  }
